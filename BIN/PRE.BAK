#include<stdio.h>
#define NUMBER 0
int main()
{
	#if NUMBER>0
		printf("\n number is positive");
	#elif NUMBER<0
		printf("\n number is negative");
	#else 
		printf("\n number is zero");
	#endif
	return 0;
}
